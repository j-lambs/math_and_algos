x <- c(0)
walk <- rbinom(n_steps - 1, 1, p)
for (i in walk) {
if (i == 1) {
x <- c(x, tail(x, 1) + 1)
}
else {
x <- c(x, tail(x, 1) - 1)
}
}
return(x)
}
